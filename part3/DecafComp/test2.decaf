class Test2 {

    real b;

    static int factorial( int n ) {
        if ( n <= 1 ) {
           return 1;
        }
        return n * factorial( n - 1 );
    }

    static void testing() {
        int n;
        n = factorial( 4 );
        writeln( n );
    }

    static void main() {
        int a;
        a = 1 * 3 + 5;
        testing();
    }
}